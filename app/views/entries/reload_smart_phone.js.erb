var $list = $('#message-list');

$list.find("#message-more").remove();

//  重なってなかったら、空にする
if ($list.find('#message<%= @entries.last.id %>').length == 0) {
    $list.empty();
}

<%  @entries.reverse.each do |entry| %>
<%    @entry = entry %>
if ($list.find('#message<%= entry.id %>').length == 0) {
    $list.prepend("<%= escape_javascript(render 'entries/entry_smart_phone') %>");
}
<% end %>
$list.append("<%= escape_javascript(render 'entries/more_smart_phone') %>");

$list.listview('refresh');
